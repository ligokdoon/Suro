<% if logged_in?%>
<script type="text/javascript">

    window.currentUser = {
         "id": <%= current_user.id %>,
          "email": "<%= current_user.email %>",
          "firstName": "<%= current_user.first_name%>",
          "lastName": "<%= current_user.last_name%>",
          "createdAt": "<%= current_user.created_at%>"
    }
    
</script>
<%end%>

<main id="root"></main>